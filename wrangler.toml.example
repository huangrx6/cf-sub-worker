name = "cf-sub-worker"
main = "src/index.js"
compatibility_date = "2024-01-01"

# 绑定 KV 命名空间
# 请将 YOUR_KV_NAMESPACE_ID 替换为你的实际 KV ID
[[kv_namespaces]]
binding = "KV"
id = "YOUR_KV_NAMESPACE_ID"

# 环境变量（可选，也可在 Dashboard 中设置）
# [vars]
# TOKEN = "your_admin_password"
# GUESTTOKEN = "your_guest_password"
